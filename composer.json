{
    "name": "mkcom/flow-sms77",
    "description": "A Flow package for easy use of MKcom.SMS77",
    "type": "neos-package",
    "require": {
        "typo3/flow": "^3.3",
        "mkcom/sms77": "^0.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:mkeitsch/mkcom.sms77.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "MKcom\\Flow\\SMS77\\": "Classes"
        }
    },
    "extra": {
        "neos": {
            "package-key": "MKcom.Flow.SMS77"
        }
    }
}
